{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4.1",
        "biscolab/laravel-recaptcha": "5.0.1",
        "botman/botman": "^2.6",
        "braintree/braintree_php": "5.5.0",
        "cronitor/cronitor-php": "^1.3",
        "currency-cloud/client": "^3.1",
        "doctrine/dbal": "^3.3",
        "dompdf/dompdf": "^1.0",
        "erdemkeren/laravel-otp": "^3.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.7",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.4",
        "laravel/passport": "7.5.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.4",
        "lcobucci/jwt": "3.3.3",
        "maatwebsite/excel": "^3.0.1",
        "mailchimp/mailchimp": "^2.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "paypal/rest-api-sdk-php": "*",
        "pusher/pusher-php-server": "*",
        "rap2hpoutre/fast-excel": "^1.7",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "razorpay/razorpay": "2.*",
        "sendgrid/sendgrid": "^8.0",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-newsletter": "^4.10",
        "spatie/laravel-permission": "4.4.0",
        "spatie/laravel-translatable": "^4.6",
        "stripe/stripe-php": "^7.92",
        "trulioo/trulioo-sdk": "^1.0",
        "twilio/sdk": "^6.20"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
